
# requirements = (CUDADeviceName == "Tesla V100-SXM2-16GB")  # optional parameter 
priority        = 0
notification	= Never
notify_user	= 
getenv		= True

name		= __main__
# main 
executable 	= /usr/bin/python3
# command line arguments
arguments  = $(name).py --save-history --evaluate --train
# optional file for stdin
input      =
# $(name).in
# optional file for stdout
output     = $(name).out
